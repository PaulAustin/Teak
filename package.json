{
  "name": "teakblocks",
  "version": "0.0.1",
  "description": "A diagram editor for teak blocks",
  "main": "teakblocks.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bifyu": "browserify source/index.js -t [ babelify --presets es2015 ] | uglifyjs >html_app/www/bundled_tbe.js; ls -l html_app/www/bundled_tbe.js",
    "bify": "browserify source/index.js -o html_app/www/bundled_tbe.js; ls -l html_app/www/bundled_tbe.js",
    "wify": "watchify source/index.js -o html_app/www/bundled_tbe.js -v",
    "abuild": "cd html_app; cordova build android",
    "ibuild": "cd html_app; cordova build iOS",
    "irun": "cd html_app; cordova run iOS",
    "cpfonts": "cp -R node_modules/font-awesome/  html_app/www/font-awesome/",
    "adeploy": "adb install -r html_app/platforms/android/build/outputs/apk/android-debug.apk",
    "http": "http-server ./html_app/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulAustin/teakblocks.git"
  },
  "keywords": [
    "graphical",
    "editor",
    "teak"
  ],
  "author": "Paul Austin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PaulAustin/teakblocks/issues"
  },
  "homepage": "https://github.com/PaulAustin/teakblocks#readme",
  "dependencies": {
    "assert": "^1.4.1",
    "font-awesome": "^4.6.3",
    "interact.js": "^1.2.6",
    "knockout": "^3.4.0",
    "teak": "0.0.11"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "eslint": "^3.4.0"
  }
}
