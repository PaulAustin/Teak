{
  "name": "teakblocks",
  "version": "0.0.1",
  "description": "A diagram editor for teak blocks",
  "main": "teakblocks.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bify": "browserify source/index.js -o html_app/www/bundled_tbe.js",
    "wify": "watchify source/index.js -o html_app/www/bundled_tbe.js -v",
    "abuild": "cd html_app; cordova build android",
    "adeploy": "adb install -r html_app/platforms/android/build/outputs/apk/android-debug.apk",
    "http": "http-server ./html_app/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulAustin/teakblocks.git"
  },
  "keywords": [
    "graphical",
    "editor",
    "teak"
  ],
  "author": "Paul Austin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PaulAustin/teakblocks/issues"
  },
  "homepage": "https://github.com/PaulAustin/teakblocks#readme",
  "dependencies": {
    "assert": "^1.4.1",
    "font-awesome": "^4.6.3",
    "interact.js": "^1.2.6",
    "webcomponents.js": "^0.7.22"
  },
  "devDependencies": {
    "browserify": "^13.1.0"
  }
}
