<?xml version='1.0' encoding='utf-8'?>
<widget id="co.trashbots.tblocks" ios-CFBundleVersion="0.1.3" version="0.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TBlocks</name>
    <description>
        A cool programmming tool.
    </description>
    <author email="info@trashbots.co" href="http://trashbots.co">
        TBlock team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="WebViewBounce" value="false" />
    <preference name="Orientation" value="landscape" />
    <platform name="android">
        <icon src="res/app-icon.png" />
        <splash src="res/screen/ios/Default@2x~universal~anyany.png" />
        <preference name="Fullscreen" value="true" />
    </platform>
    <platform name="ios">
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-20.png" height="20" width="20"/>
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-20@2x.png" height="40" width="40"/>
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-20@3x.png" height="60" width="60" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-29.png" width="29" height="29" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-29@2x.png" height="58" width="58" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-29@3x.png" height="87" width="87" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-40.png" height="40" width="40" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-40@2x.png" height="80" width="80" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-40@3x.png" height="120" width="120" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-60@2x.png" height="120" width="120" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-60@3x.png" height="180" width="180" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-76.png" height="76" width="76" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-76@2x.png" height="152" width="152" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-83.5@2x.png" height="167" width="167" />
        <icon src="res/ios/AppIcon.appiconset/TBlocksIcon-1024.png" height="1024" width="1024" />
        <splash src="res/ios/Default@2x~universal~anyany.png" />
        <splash src="res/ios/Default@2x~universal~comany.png" />
        <splash src="res/ios/Default@2x~universal~comcom.png" />
    </platform>
    <plugin name="cordova-plugin-ble" spec="1" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="phonegap-plugin-mobile-accessibility" spec="git+https://github.com/phonegap/phonegap-mobile-accessibility.git" />
    <engine name="android" spec="^7.1.4" />
    <engine name="ios" spec="~4.5.5" />
</widget>
